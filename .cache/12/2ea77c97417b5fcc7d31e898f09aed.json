);\r\n\tz-index: 2;\r\n\tposition: relative;\r\n\tborder-bottom: 2px solid #444;\r\n\tjustify-content: space-between;\r\n}\r\n\r\nnav.sub {\r\n\tbox-shadow: 0px 0px 4px 4px black;\r\n}\r\n\r\n.logo {\r\n\tbackground-image: url(\"../../gfx/icon128.png\");\r\n\tbackground-size: contain;\r\n\twidth: 36px;\r\n\theight: 36px;\r\n\tmargin: 0 0.5em;\r\n}\r\n\r\n.separator {\r\n\twidth: 2px;\r\n\theight: 80%;\r\n\tbackground-color: #ccc;\r\n}\r\n\r\n.btn,\r\n.label {\r\n\theight: 24px;\r\n\tmargin: 4px;\r\n\tborder-radius: 4px;\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\tfont-size: 16px;\r\n\tuser-select: none;\r\n}\r\n\r\n.left {\r\n\tjustify-self: flex-start;\r\n}\r\n\r\n.right {\r\n\tjustify-self: flex-end;\r\n}\r\n\r\n.btn {\r\n\tmin-width: 100px;\r\n\tcursor: pointer;\r\n}\r\n\r\n.btn:hover,\r\n.btn.active {\r\n\tbackground-color: #ccc;\r\n\tcolor: #222;\r\n\ttransition: color 0.5s, background-color 0.5s;\r\n}\r\n\r\n.isOn,\r\n.isOff {\r\n\tdisplay: none;\r\n}\r\n\r\n.active > .isOn {\r\n\tdisplay: initial;\r\n}\r\n\r\n:not(.active) > .isOff {\r\n\tdisplay: initial;\r\n}\r\n\r\n.red {\r\n\tcolor: #cc2222;\r\n}\r\n\r\n.green {\r\n\tcolor: #22cc22;\r\n}\r\n\r\n.btn.locked {\r\n\tpointer-events: none;\r\n\topacity: 0.5;\r\n}\r\n\r\n.material-icons {\r\n\tmargin-left: 0.5em;\r\n}\r\n\r\n.btn.active > .blink {\r\n\tanimation: blink-anim 1s infinite alternate;\r\n}\r\n\r\nsection.tab {\r\n\tdisplay: none;\r\n}\r\n\r\nsection.tab.active {\r\n\tdisplay: block;\r\n}\r\n\r\n#online_status {\r\n\tfont-weight: bolder;\r\n\tpadding: 0.5em;\r\n\tword-wrap: normal;\r\n}\r\n\r\n.list {\r\n\twidth: 100%;\r\n\theight: calc(100vh - var(--root-nav) - var(--nav));\r\n\tdisplay: flex;\r\n\tflex-direction: column;\r\n\tfont-family: MONOSPACE;\r\n\toverflow: auto;\r\n}\r\n\r\n.list > .item {\r\n\tdisplay: block;\r\n\theight: 20px;\r\n\tborder-bottom: 1px solid #444;\r\n\tline-height: 20px;\r\n\tcolor: #ccc;\r\n\tfont-size: 12px;\r\n}\r\n\r\n.item > .ticker {\r\n\tmargin: 0 1em 0 0.